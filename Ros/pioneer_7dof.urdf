                </geometry>
               <origin xyz=" 0.0 -0.125 0.0" rpy ="1.5708 0.0 0.0"/>
            </collision>
        </link>
        <joint name="One_joint" type = "continuous">
            <parent link ="base_link"/>
            <child link ="One"/>
            <axis xyz = "0.0 1.0 0.0"/>
            <origin xyz = "0.0 0.0 0.3" rpy = "1.5708 0.0 1.5708"/>
        </joint>

