<robot name="robotic_arm">
    <link name="world"/>
    <!--ONE-->
        <link name="base_link">
            <visual>
                <geometry>
                    <box size="0.2 0.2 0.1"/>
                </geometry>
                <material name = "Blue">
                    <color rgba ="0.0 0.0 0.8 1.0"/>
                </material>
                <origin xyz=" 0.0 0.0 0.05" rpy ="0.0 0.0 0.0"/>
            </visual>
            <collision>
                <geometry>
